<Application x:Class="DragAndDrop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        
        <SolidColorBrush x:Key="DroppableAdornerBorderBrush" Color="White" Opacity="0.5" />
        <SolidColorBrush x:Key="DroppableAdornerBackgroundBrush" Color="Blue" Opacity="0.3" />
        <SolidColorBrush x:Key="DroppableAdornerLineBrush" Color="Blue" />
        
    </Application.Resources>
</Application>
